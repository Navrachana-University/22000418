MISSION {
    PokeInt i = 1;
    PokeInt num = 0;
    PokeInt fact = 0;
    PokeInt j = 0;

    TRAIN (PokeInt i = 1; i <= 5; i = i + 1) {
        num = i;
        fact = 1;

        TRAIN (PokeInt j = 1; j <= num; j = j + 1) {
            fact = fact * j;
        }

        CALL "Factorial calculated!";
    }
}